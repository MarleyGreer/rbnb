<h1 class="header">Make A Booking </h1>
<div class="container w-50 mb-3">
  <%= simple_form_for [@garment, @booking] do |f| %>
    <%= f.input :start_date, label: 'Start Date'%>
    <%= f.input :end_date, label: 'End Date'%>
    <%= f.button :submit, class: "btn-dark search-cocktails mb-3"%>
  <% end %>

